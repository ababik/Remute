<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.4;net45</TargetFrameworks>
    <Authors>Alexey Babik</Authors>
    <Company />
    <Description>.NET tool to produce new immutable object applying lambda expressions to existing immutable object</Description>
    <Copyright>Copyright © Alexey Babik 2018</Copyright>
    <PackageProjectUrl>https://github.com/ababik/Remute</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ababik/Remute</RepositoryUrl>
    <PackageTags>immutable lambda expression</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.6</Version>
    <RootNamespace>Remutable</RootNamespace>
    <AssemblyVersion>1.0.6.0</AssemblyVersion>
  </PropertyGroup>

</Project>
